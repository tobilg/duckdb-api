{
  "name": "duckdb-api",
  "version": "0.1.0",
  "description": "A DuckDB API",
  "type": "module",
  "scripts": {
    "build": "rm -rf dist/ && tsc --build src/",
    "dev": "nodemon --exec tsx src/index.ts",
    "serve": "nodemon --exec tsx src/index.ts",
    "format": "npx @biomejs/biome format --write src/",
    "lint": "npx @biomejs/biome lint --write --unsafe src/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.5",
    "@hono/zod-openapi": "^0.17.0",
    "bunyan": "^1.8.15",
    "duckdb": "^1.1.1",
    "hono": "^4.6.9",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bunyan": "^1.8.11",
    "@types/node": "^22.9.0",
    "nodemon": "^3.1.7"
  }
}

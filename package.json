{
  "name": "duckdb-api",
  "version": "0.1.1",
  "description": "A DuckDB API",
  "type": "module",
  "scripts": {
    "build": "rm -rf dist/ && tsc --build src/",
    "dev": "nodemon --exec tsx src/index.ts",
    "serve": "nodemon --exec tsx src/index.ts",
    "format": "npx @biomejs/biome format --write src/",
    "lint": "npx @biomejs/biome lint --write --unsafe src/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.8",
    "bunyan": "^1.8.15",
    "duckdb": "^1.2.0",
    "hono": "^4.7.4",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bunyan": "^1.8.11",
    "@types/node": "^22.13.10",
    "nodemon": "^3.1.9"
  }
}
